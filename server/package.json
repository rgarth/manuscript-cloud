{
  "name": "manuscript-cloud-server",
  "version": "1.0.0",
  "description": "Manuscript Cloud API Server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "cleanup": "tsx src/scripts/cleanup-mongo.ts",
    "cleanup:reset": "tsx src/scripts/cleanup-mongo.ts reset --confirm",
    "cleanup:stats": "tsx src/scripts/cleanup-mongo.ts stats"
  },
  "keywords": [
    "manuscript",
    "writing",
    "google-docs"
  ],
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "googleapis": "^149.0.0",
    "mongoose": "^8.15.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.23",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}